% generate
global sporco_path;

S0 = single(stdimage('lena.grey')) / 255;
S0 = imresize(S0,.5);
S0 = S0+.04*randn(size(S0));


% load clean dictionaries
load('CacheData/Dict_12x12x35.mat');
D0 = D;



%%%%%%%%%%%%%%%%%%%%%%%%%%  First Batch  %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

lambda_all = [.1,.15,.2,.25,.3,.4];


num_dict = 6:4:35;

disp(['batch 1']);

for i = 1:length(num_dict)
    for j = 1:length(lambda_all)
        disp([num2str(i), ',', num2str(j)]);
        n = num_dict(i);
        D_init = D0(:,:,1:n);
        lambda = lambda_all(j);
        imind = 1;
        maxit = 300;
        [D1,X1,D2,X2] = exp_dict_4(D_init,lambda,imind,maxit);
        fname = ([sporco_path,'/Graph/CacheData/DictNoisyLenaCbpdn1/DictComp',...
            num2str(i),num2str(j),'.mat']);
        save(fname,'D2','X2','lambda','n','D1','X1','mu');
    end
end



